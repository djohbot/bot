{
    "name": "djohbot",
    "version": "1.2.0",
    "description": "djoh's random discord bot",
    "main": "build/sharding",
    "scripts": {
        "build": "yarn clean && tsc",
        "build:run": "yarn build && yarn start",
        "build:watch": "tsc -w",
        "clean": "rm -rf build tsconfig.tsbuildinfo",
        "dev": "tsc-watch --noClear --onSuccess \"yarn start\"",
        "start": "node --no-warnings ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/djohbot/bot.git"
    },
    "author": "djoh",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/djohbot/bot/issues"
    },
    "homepage": "https://github.com/djohbot/bot",
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "@typegoose/typegoose": "^10.1.0",
        "axios": "^1.3.2",
        "dedent": "^0.7.0",
        "discord-hybrid-sharding": "^2.1.0",
        "discord.js": "^14.7.1",
        "dotenv": "^16.0.3",
        "fastify": "^4.12.0",
        "i18next": "^22.4.9",
        "lodash": "^4.17.21",
        "mongoose": "~6.9.0",
        "pretty-ms": "^7.0.1",
        "recursive-readdir": "^2.2.3",
        "svg2img": "^1.0.0-beta.2",
        "text-table": "^0.2.0",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@types/dedent": "^0.7.0",
        "@types/lodash": "^4.14.191",
        "@types/node": "^18.11.18",
        "@types/recursive-readdir": "^2.2.1",
        "@types/text-table": "^0.2.2",
        "copyfiles": "^2.4.1",
        "tsc-watch": "^6.0.0",
        "typescript": "^5.0.0"
    }
}
